{
  "build": {
    "arduino": {
      "ldscript": "esp32s2_out.ld"
    },
    "core": "esp32",
    "extra_flags": [
      "-DBUSWARE_S2",
      "-DBOARD_HAS_PSRAM",
      "-DARDUINO_USB_MSC_ON_BOOT=1",
      "-DARDUINO_USB_CDC_ON_BOOT=1"
    ],
    "f_cpu": "240000000L",
    "f_flash": "80000000L",
    "flash_mode": "dio",
    "hwids": [
      [
        "0X303A",
        "0x80C5"
      ]
    ],
    "mcu": "esp32s2",
    "variant": "busware32s2"
  },
  "connectivity": [
    "wifi"
  ],
  "debug": {
    "openocd_board": "esp32s2.cfg"
  },
  "frameworks": [
    "arduino",
    "espidf"
  ],
  "name": "BUSWARE ESP32S2",
  "upload": {
    "flash_size": "4MB",
    "maximum_ram_size": 327680,
    "maximum_size": 4194304,
    "use_1200bps_touch": true,
    "wait_for_upload_port": true,
    "require_upload_port": true,
    "speed": 921600
  },
  "url": "https://busware.de/",
  "vendor": "busware.de"
}
